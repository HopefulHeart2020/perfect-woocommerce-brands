{
	"name": "quadlayers/perfect-woocommerce-brands",
	"description": "Perfect Brands for WooCommerce",
	"type": "project",
	"authors": [
		{
			"name": "QuadLayers",
			"email": "noreply@quadlayers.com"
		}
	],
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"automattic/jetpack-composer-plugin": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require": {
		"automattic/jetpack-composer-plugin": "*",
		"automattic/jetpack-assets": "*",
		"quadlayers/wp-dashboard-widget-news": "*",
		"quadlayers/wp-notice-plugin-required": "*",
		"quadlayers/wp-notice-plugin-promote": "*",
		"quadlayers/wp-plugin-table-links": "*",
		"quadlayers/wp-plugin-suggestions": "*"
	},
	"autoload": {
		"classmap": [
			"lib/",
			"src/"
		]
	},
	"extra": {
		"quadlayers/autoload": {
			"\\QuadLayers\\XXX\\": "lib/",
			"\\QuadLayers\\XXX\\": "src/"
		},
		"wp-plugin-slug": "perfect-woocommerce-brands"
	},
	"require-dev": {
		"woocommerce/woocommerce-sniffs": "^0.1.3",
		"phpunit/phpunit": "^9.5"
	},
	"scripts": {
		"test:phpunit": "./vendor/bin/phpunit phpunit"
	}
}